<%- form_tag(url, options) %>
	<% if (typeof error !== "undefined") { %>
		<div id="error_explanation">
			<h2><%= pluralize(Object.keys(error).length, "error") %> prohibited this %%model%% from being saved:</h2>

			<ul>
			<% for (var k in error) { %>
				<li><%= error[k] %></li>
			<% } %>
			</ul>
		</div>
	<% } %>

%%form_html%%
	<div class="actions">
		<%- submit_tag ("Save") %>
	</div>
<%- form_end() %>
